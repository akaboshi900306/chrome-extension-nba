{
  "manifest_version": 2,

  "name": "NBA BOX SCORE",
  "description": "This extension shows you the box score of NBA games.",
  "version": "1.0",


  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
        "storage",
        "http://stats.nba.com/",
        "http://data.nba.com/"
    ]
}