{
  "name": "chrome-extension-nba",
  "version": "1.2.5",
  "description": "This is a browser extension shows the daily box score for NBA games.",
  "author": "Liu, Ming Chia",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "webpack-dev-server --open",
    "start:dev": "NODE_ENV=development webpack-dev-server --open",
    "build": "npm test && cd src && web-ext build --overwrite-dest",
    "lint": "npm-run-all lint:*",
    "lint:addon": "addons-linter src --self-hosted",
    "lint:css": "stylelint src/css/*.css",
    "lint:js": "eslint src",
    "test": "npm run lint",
    "webpack": "NODE_ENV=production webpack --progress --colors",
    "webpack:dev": "NODE_ENV=development webpack --progress --colors"
  },
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mingchia-andy-liu/chrome-extension-nba.git"
  },
  "bugs": {
    "url": "https://github.com/mingchia-andy-liu/chrome-extension-nba/issues"
  },
  "homepage": "https://github.com/mingchia-andy-liu/chrome-extension-nba#readme",
  "devDependencies": {
    "addons-linter": "^0.41.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "html-webpack-plugin": "^3.2.0",
    "htmllint-cli": "0.0.7",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "web-ext": "^2.6.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
  }
}
